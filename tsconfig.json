{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "noEmit": false,
    "noImplicitAny": false,
    "outDir": "./dist/",
    "paths": {},
    "rootDir": "./src",
    "sourceMap": true,
    "target": "es5"
  },
  "include": ["./src/**/*"]
}
